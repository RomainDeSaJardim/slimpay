{
    "parser": "babel-eslint",
    "env": {
        "node": true
    },
    "rules": {
        "quotes": [2, "single"],
        "eol-last": [0],
        "no-mixed-requires": [0],
        "no-underscore-dangle": [0],
        "no-unused-vars": [2, {"vars": "all", "args": "none"}],
        "no-use-before-define": [2, "nofunc"],
        "curly": [0]
    },
    "ecmaFeatures": {
        "modules": true
    }
}
